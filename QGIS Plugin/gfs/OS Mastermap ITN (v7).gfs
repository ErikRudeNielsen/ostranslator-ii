      <Type>StringList</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>reasonForChange</Name>
      <ElementPath>changeHistory|reasonForChange</ElementPath>
      <Type>StringList</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>roadNameCy</Name>
      <ElementPath>roadName</ElementPath>
      <Condition>@xml:lang='cy'</Condition>
      <Type>String</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>roadNameGd</Name>
      <ElementPath>roadName</ElementPath>
      <Condition>@xml:lang='gd'</Condition>
      <Type>String</Type>
    </PropertyDefn>
    <!-- the next element should capture empty lang attribute (defaults 
    to en and also those with values of en -->
    <PropertyDefn>
      <Name>roadNameEn</Name>
      <ElementPath>roadName</ElementPath>
      <Condition>@xml:lang!='cy' and @xml:lang!='gd'</Condition>
      <Type>String</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>theme</Name>
      <ElementPath>theme</ElementPath>
      <Type>StringList</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>version</Name>
      <ElementPath>version</ElementPath>
      <Type>Integer</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>versionDate</Name>
      <ElementPath>versionDate</ElementPath>
      <Type>String</Type>
    </PropertyDefn>
  </GMLFeatureClass>
