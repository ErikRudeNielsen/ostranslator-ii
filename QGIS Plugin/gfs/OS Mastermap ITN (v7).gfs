  <GMLFeatureClass>
    <Name>Road</Name>
    <ElementPath>Road</ElementPath>
    <!-- TODO The bounding box is not pulled from the gml - this can be 
    updated with a post-load SQL query. -->
    <!-- <GeometryElementPath>boundedBy</GeometryElementPath> -->
    <GeometryType>3</GeometryType>
    <SRSName>osgb:BNG</SRSName>
    <!-- The fid attribute is found automatically and we do not need to 
    define it as such -->
    <!-- <PropertyDefn>
      <Name>fid</Name>
      <ElementPath>@fid</ElementPath>
      <Type>String</Type>
      <Length>20</Length>
    </PropertyDefn> -->
    <PropertyDefn>
