<GMLFeatureClassList>
  <GMLFeatureClass>
    <Name>Street</Name>
    <ElementPath>Street</ElementPath>
    <GeomPropertyDefn>
      <Name>streetStart</Name>
      <ElementPath>streetStart</ElementPath>
      <Type>Point</Type>
      <SRSName>EPSG:27700</SRSName>
    </GeomPropertyDefn>
    <GeomPropertyDefn>
      <Name>streetEnd</Name>
      <ElementPath>streetEnd</ElementPath>
      <Type>Point</Type>
      <SRSName>EPSG:27700</SRSName>
    </GeomPropertyDefn>
    <PropertyDefn>
      <Name>usrn</Name>
      <ElementPath>usrn</ElementPath>
      <Type>Integer</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>recordType</Name>
      <ElementPath>recordType</ElementPath>
      <Type>Integer</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>swaOrgRefNaming</Name>
      <ElementPath>swaOrgRefNaming</ElementPath>
      <Type>Integer</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>state</Name>
      <ElementPath>state</ElementPath>
      <Type>Integer</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>stateDate</Name>
      <ElementPath>stateDate</ElementPath>
      <Type>String</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>streetSurface</Name>
      <ElementPath>streetSurface</ElementPath>
      <Type>Integer</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>streetClassification</Name>
      <ElementPath>streetClassification</ElementPath>
      <Type>Integer</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>version</Name>
      <ElementPath>version</ElementPath>
      <Type>Integer</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>streetTolerance</Name>
      <ElementPath>streetTolerance</ElementPath>
      <Type>Integer</Type>
    </PropertyDefn>
    <!-- The following properties are taken from the StreetDescriptiveIdentifier(s) -->
    <PropertyDefn>
      <Name>streetDescription</Name>
      <ElementPath>streetDescriptiveIdentifierMember|StreetDescriptiveIdentifier|streetDescription</ElementPath>
      <Type>StringList</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>localityName</Name>
      <ElementPath>streetDescriptiveIdentifierMember|StreetDescriptiveIdentifier|localityName</ElementPath>
      <Type>StringList</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>townName</Name>
      <ElementPath>streetDescriptiveIdentifierMember|StreetDescriptiveIdentifier|townName</ElementPath>
      <Type>StringList</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>administrativeArea</Name>
      <ElementPath>streetDescriptiveIdentifierMember|StreetDescriptiveIdentifier|administrativeArea</ElementPath>
      <Type>StringList</Type>
    </PropertyDefn>
  </GMLFeatureClass>
</GMLFeatureClassList>
